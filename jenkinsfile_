pipeline{
  agent any
   stages{
      stage('build'){
	     step{
		    echo "This is the building stage" 
		 }
	  stage('test'){
	      step{
		     echo "This is the testing step"
		  }
	   stage('deploy'){
	       step{
		      echo "This is the deployment stage"
		   }
	   
	   }
	  } 
	  
	  }
   
   
   }

}
